{
    "name": "lucidtaz/yii2-whoops",
    "description": "Yii2 bindings for Whoops error handler",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "^2.0.13",
        "filp/whoops": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": { "lucidtaz\\yii2whoops\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "lucidtaz\\yii2whoops\\tests\\": "tests/" }
    },
    "authors": [
        {
            "name": "Thijs Zumbrink",
            "email": "thijs@schalpoen.nl"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
